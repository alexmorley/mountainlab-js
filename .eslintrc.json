{
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "env" : {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "require-jsdoc": "off",
        "no-console": "off",
        "camelcase": "off",
        "guard-for-in": "warn",
        "max-len": "warn",
        "new-cap": "warn"
    }
}
